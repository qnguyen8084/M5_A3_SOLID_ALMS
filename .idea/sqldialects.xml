<?xml version="1.0" encoding="UTF-8"?>
<!-- Root element for the project configuration file -->
<project version="4">
  <!-- Component that handles SQL dialect mappings for the project -->
  <component name="SqlDialectMappings">
    
    <!-- Maps the directory containing JDBC drivers to the MySQL SQL dialect -->
    <file url="file://$APPLICATION_CONFIG_DIR$/jdbc-drivers" dialect="MySQL" />
    
    <!-- Maps the whole project to the generic SQL dialect -->
    <file url="PROJECT" dialect="GenericSQL" />
    
  </component>
</project>
